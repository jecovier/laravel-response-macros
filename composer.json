{
    "name": "jecovier/laravel-response-macros",
    "description": "Extra Response Macro's for Laravel",
    "keywords": [
        "jecovier",
        "fractal",
        "api",
        "appstract",
        "laravel",
        "response",
        "macros",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Espinoza Conforme",
            "email": "jecovier@gmail.com",
            "homepage": "https://github.com/jecovier",
            "role": "Developer"
        },
        {
            "name": "Bob Krijnen",
            "email": "hello@appstract.team",
            "homepage": "https://appstract.team",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Appstract\\ResponseMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appstract\\ResponseMacros\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appstract\\ResponseMacros\\ResponseMacrosServiceProvider"
            ]
        }
    }
}
